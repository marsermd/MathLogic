#match x+y=y+x->x+y'=y'+x
#deduct 1
x+y=y+x|-x+y'=y'+x

x+y'=(x+y)'

#use ..\g)x'+y=(x+y)'; y'+x=(y+x)'

x+y=y+x -> (x+y)'=(y+x)'
x+y=y+x
(x+y)'=(y+x)'

#use ..\c)t=r→r=s→t=s; x+y'=(x+y)'->(x+y)'=(y+x)'->x+y'=(y+x)'
(x+y)'=(y+x)'->x+y'=(y+x)'
x+y'=(y+x)'

#use ..\d)r=t→s=t→r=s; x+y'=(y+x)'->y'+x=(y+x)'->x+y'=y'+x
y'+x=(y+x)'->x+y'=y'+x
x+y'=y'+x